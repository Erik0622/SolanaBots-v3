{
  "buildCommand": "prisma generate && next build",
  "build": {
    "env": {
      "PRISMA_CLI_BINARY_TARGETS": "native",
      "DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/solana_bots?schema=public"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
} 